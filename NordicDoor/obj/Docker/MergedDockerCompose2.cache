name: dockercompose14537871168346289203
services:
  nordicdoor:
    build:
      context: /Users/ThomasTvedten/Documents/nordicdoor/nordicdoor/NordicDoor
      dockerfile: NordicDoor/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: NordicDoor
      target: base
    container_name: NordicDoor
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: nordicdoor:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/NordicDoor.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: /Users/mariusfjermeros/Documents/GitHub/nordicdoor/NordicDoor/NordicDoor
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: /Users/mariusfjermeros/Documents/GitHub/nordicdoor/NordicDoor
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: /Users/mariusfjermeros/vsdbg/vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: /Users/mariusfjermeros/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose14537871168346289203_default