@* Returns a temporary message to the user if add, edit or delete was successfull*@

@if (TempData["suksess"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script type="text/javascript">
            toastr.success('@TempData["suksess"]');
    </script>
}
    @** Returns a temporary errormessage to the user if add, edit or delete fails *@ 

@if (TempData["feilet"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script type="text/javascript">
            toastr.error('@TempData["feilet"]');
    </script>
}

